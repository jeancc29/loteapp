<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/btnTestBT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="testprinterbt2" />

    <Button
        android:id="@+id/btnTestBLE"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="testprinterbt4" />

    <Button
        android:id="@+id/btnTestNET"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="testprinternet" />

    <Button
        android:id="@+id/btnTestUSB"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="testprinterusb" />

    <Button
        android:id="@+id/btnImagen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Imagen ver" />

    <TextView
        android:id="@+id/textViewTicketType"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="tickettype" />

    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <RadioButton
            android:id="@+id/radioButtonTicket58"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="tickettype58mm" />

        <RadioButton
            android:id="@+id/radioButtonTicket80"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="tickettype80mm" />
    </RadioGroup>

    <TextView
        android:id="@+id/textViewHardware"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="hardwarefunction" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <CheckBox
            android:id="@+id/checkBoxCutter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="cutter" >
        </CheckBox>

        <CheckBox
            android:id="@+id/checkBoxDrawer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="drawer" >
        </CheckBox>

        <CheckBox
            android:id="@+id/checkBoxBeeper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="beeper" >
        </CheckBox>
    </LinearLayout>

    <TextView
        android:id="@+id/textViewPrintCount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="printcount" />

    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <RadioButton
            android:id="@+id/radioButtonPrintCount1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="count1" />

        <RadioButton
            android:id="@+id/radioButtonPrintCount10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="count10" />

        <RadioButton
            android:id="@+id/radioButtonPrintCount100"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="count100" />

        <RadioButton
            android:id="@+id/radioButtonPrintCount1000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="count1000" />
    </RadioGroup>

    <TextView
        android:id="@+id/textViewPrintContent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="printcontent" />

    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <RadioButton
            android:id="@+id/radioButtonPrintContentS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="small" />

        <RadioButton
            android:id="@+id/radioButtonPrintContentM"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="middle" />

        <RadioButton
            android:id="@+id/radioButtonPrintContentL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="large" />
    </RadioGroup>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <CheckBox
            android:id="@+id/checkBoxAutoPrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="autoprint" >
        </CheckBox>

        <CheckBox
            android:id="@+id/checkBoxPictureCompress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="bitmapcompress" >
        </CheckBox>

        <CheckBox
            android:id="@+id/checkBoxCheckReturn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="checkreturn" >
        </CheckBox>
    </LinearLayout>

</LinearLayout>